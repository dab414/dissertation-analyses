library(ez)
m1 <- ezANOVA(wid = subject, within = .(difference, difficulty), dv = choiceRt, data = d, detailed = TRUE)
m1 <- cbind(m1$ANOVA, data.frame(n2p = m1$ANOVA$SSn / (m1$ANOVA$SSn + m1$ANOVA$SSd)))
m1
