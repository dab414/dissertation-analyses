scale_x_continuous(breaks = seq(.1,.9,.2), labels = seq(.1,.9,.2)) +
scale_color_discrete(name = 'Model', labels = c(linear = 'Linear', powerfun = 'Power')) +
labs(
x = 'Chosen Deck Probability of Switching',
y = 'Response Time (ms)'
) +
theme_bw() +
theme(legend.position = c(.1,.8))
